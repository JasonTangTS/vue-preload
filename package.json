  },
  "homepage": "https://github.com/egoist/vue-preload#readme",
  "devDependencies": {
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "cssnext": "^1.8.4",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "superagent": "^1.4.0",
    "vue": "^1.0.8"
